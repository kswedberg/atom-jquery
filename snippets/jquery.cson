".source.js":
  "$ (Select or Create DOM Element)":
    prefix: "$"
    body: "\\$(${1:string/element/array/function/jQueryObject/string})$0"
  "$(this)":
    prefix: "this"
    body: "$(this)$0"

  "document ready (safe)":
    prefix: "readyjQuery"
    body: '''
      jQuery(document).ready(function($) {
      	${0: // Stuff to do as soon as the DOM is ready. Use $() w/o colliding with other libs}
      });

    '''
  "document ready":
    prefix: "ready"
    body: '''
      $(document).ready(function() {
      	${0: // Stuff to do as soon as the DOM is ready}
      });

    '''
  "$.holdReady":
    prefix: "holdReady"
    body: "$.holdReady(${1:hold})$0"
  "$.holdReady chain":
    prefix: ".holdReady"
    body: ".holdReady(${1:hold})$0"

  "$.readyException":
    prefix: "readyException"
    body: "$.readyException(${1:error})$0"
  "$.readyException chain":
    prefix: ".readyException"
    body: ".readyException(${1:error})$0"

  "$.escapeSelector":
    prefix: "escapeSelector"
    body: "$.escapeSelector(${1:selector})$0"
  "$.escapeSelector chain":
    prefix: ".escapeSelector"
    body: ".escapeSelector(${1:selector})$0"

  "$.htmlPrefilter":
    prefix: "htmlPrefilter"
    body: "$.htmlPrefilter(${1:html})$0"
  "$.htmlPrefilter chain":
    prefix: ".htmlPrefilter"
    body: ".htmlPrefilter(${1:html})$0"

  "$.noConflict":
    prefix: "noConflict"
    body: "\\$.noConflict(${1:removeAll})$0"
  "$.noConflict chain":
    prefix: ".noConflict"
    body: ".noConflict(${1:removeAll})$0"

  get:
    prefix: "getjq"
    body: "get(${1:element index})$0"
  index:
    prefix: "index"
    body: "index(${1:selector/element}})$0"
  toArray:
    prefix: "toArray"
    body: "toArray()$0"
".text.html":
  "<script> (code.jquery.com)":
    prefix: "jquerycdn"
    body: "<script src=\"http://code.jquery.com/jquery${1:-version}${2:.min}.js\"></script>"
